{
  "name" : "ops-talent/securitybundle",
  "description" : "REST API security bundle compatible with opstalent/apibundle",
  "type" : "symfony-bundle",
  "authors" : [
    {
      "name" : "Szymon Kunowski",
      "email" : "szymon.kunowski@opstalent.it",
      "homepage": "https://github.com/auamarto"
    },
    {
      "name" : "Tomasz Piasecki",
      "email" : "tomasz.piasecki@opstalent.it",
      "homepage": "https://github.com/Mojihito"
    }
  ],
  "keywords" : [
    "api", "restful", "symfony 3", "redis", "elasticsearch", "security"
  ],
  "license" : [
    "MIT"
  ],
  "require" : {
    "opstalent/apibundle": "dev-master",
    "symfony/event-dispatcher": "^3.2",
    "symfony/form": "^3.2",
    "symfony/security": "^3.2"
  },
  "suggest": {
  },
  "autoload" : {
    "psr-0" : {
      "Opstalent\\SecurityBundle" : ""
    }
  },
  "autoload": {
    "psr-4": {
      "Opstalent\\SecurityBundle\\" : ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "repositories" : [{
    "type" : "vcs",
    "url" : "https://github.com/opstalent/security-bundle.git"
  }]

}
